<!DOCTYPE html>
<html lang="en">
  <%- include('./partials/head.ejs') %>
  <body>
    <%- include('./partials/navbar.ejs) %>
    <div class="container">
      <div class="content">
        <% if (users.length > 0) { %> <%user.forEach(user => { %>

        <h3 class="title"><%= user.firtName %> <% = user.lastName$></h3>

        <p><% = user.email %></p>

        <% }) %> <% } else {%>
        <p>Não há usuarios para exibir!!!</p>
        <% } %>
      </div>
    </div>
  </body>
</html>
